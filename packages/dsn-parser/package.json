{
  "name": "@soluble/dsn-parser",
  "description": "Utility parser for DSN",
  "version": "1.3.2",
  "license": "MIT",
  "homepage": "https://github.com/soluble-io/cache-interop/tree/main/packages/dsn-parser",
  "repository": {
    "type": "git",
    "url": "https://github.com/soluble-io/cache-interop.git",
    "directory": "packages/dsn-parser"
  },
  "author": {
    "name": "Vanvelthem SÃ©bastien",
    "url": "https://github.com/belgattitude"
  },
  "keywords": [
    "cache",
    "interop",
    "dsn",
    "parser",
    "redis",
    "ioredis"
  ],
  "jsnext:main": "dist/dsn-parser.es.js",
  "source": "src/index.ts",
  "module": "dist/dsn-parser.es.js",
  "main": "dist/dsn-parser.js",
  "umd:main": "dist/dsn-parser.umd.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "start": "cross-env NODE_ENV=development microbundle watch",
    "build": "rimraf ./dist && cross-env NODE_ENV=production microbundle --tsconfig tsconfig.build.json --format esm,cjs,umd --target node --compress",
    "?build-publishable-release": "When https://github.com/atlassian/changesets/issues/432 has a solution we can remove this trick",
    "build-publishable-release": "yarn build",
    "check-dist": "es-check -v",
    "clean": "rimraf --no-glob ./dist ./build ./coverage ./out",
    "test-unit": "jest --config jest.config.js --color $@",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "typecheck-build": "tsc --project tsconfig.build.json --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "fix-staged": "lint-staged --allow-empty",
    "coverage-publish": "codecov --root=../../ -F dsn_parser_unit"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{yaml,json,md,html}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "cross-env": "7.0.3",
    "es-check": "6.2.1",
    "jest": "27.5.1",
    "microbundle": "0.14.2",
    "rimraf": "3.0.2",
    "ts-jest": "27.1.3",
    "tslib": "2.3.1",
    "typescript": "4.5.5"
  },
  "browserslist": [
    ">0.5%",
    "not IE 11",
    "not dead",
    "not op_mini all",
    "node 12.14"
  ],
  "engines": {
    "node": "^12.14.0 || ^14.13.1 || >=16"
  }
}
