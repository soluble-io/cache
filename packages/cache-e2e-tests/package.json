      "eslint --fix"
    ],
    "*.{yaml,json,md,html}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@soluble/cache-interop": "workspace:^",
    "@soluble/cache-ioredis": "workspace:^",
    "@soluble/cache-redis": "workspace:^",
    "@soluble/dsn-parser": "workspace:^",
    "@types/ioredis": "4.28.10",
    "@types/jest": "27.5.2",
    "@types/redis": "2.8.32",
    "ioredis": "5.0.6",
    "jest": "28.1.0",
    "redis": "3.1.2",
    "testcontainers": "8.10.1",
    "ts-jest": "28.0.3",
    "typescript": "4.7.2"
